{
  "name": "nv-trung/laraplans",
  "description": "SaaS style recurring plans for Laravel.",
  "keywords": [
    "plans",
    "laravel",
    "subscriptions",
    "memberships"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nv-trung/laraplans",
  "support": {
    "issues": "https://github.com/nv-trung/laraplans/issues",
    "source": "https://github.com/nv-trung/laraplans"
  },
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "nesbot/carbon": "^2.0",
    "illuminate/database": "^8.2",
    "illuminate/support": "^8.2"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1",
    "orchestra/testbench-core": "^6.5",
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Gerardojbaez\\Laraplans\\": "src/Laraplans/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Gerardojbaez\\Laraplans\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
